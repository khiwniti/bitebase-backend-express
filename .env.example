# BiteBase Backend Environment Configuration
# Copy this file to .env.development or .env.production and fill in your values

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/bitebase

# Foursquare API Configuration
FOURSQUARE_API_KEY=fsq_your_foursquare_api_key_here
FOURSQUARE_RATE_LIMIT=100000

# Redis Configuration (for caching)
REDIS_URL=redis://localhost:6379

# Mapbox Configuration (existing)
MAPBOX_API_KEY=pk.your_mapbox_api_key_here

# AWS Bedrock Configuration (existing)
BEDROCK_API_BASE_URL=your_bedrock_gateway_url
BEDROCK_CHAT_MODEL=claude-3-sonnet
BEDROCK_REASONING_MODEL=claude-3.5-sonnet
BEDROCK_FAST_MODEL=claude-3-haiku

# Application Configuration
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:3000

# Security Configuration
JWT_SECRET=your_jwt_secret_here

# Logging Configuration
LOG_LEVEL=info