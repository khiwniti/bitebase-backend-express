{
  "name": "bitebase-restack-agents",
  "version": "1.0.0",
  "description": "BiteBase AI Agents powered by Restack.io",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "dev:watch": "ts-node --watch src/index.ts",
